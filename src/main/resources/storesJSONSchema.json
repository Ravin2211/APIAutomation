{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "storeId": { "type": "string" },
      "storeName": { "type": "string" },
      "owner": { "type": "string" },
      "address": { "type": "string" },
      "price": { "type": "string", "pattern": "^[0-9]+(\\.[0-9]{2})?$" },
      "contactNumber": { "type": "string" },
      "id": { "type": "string" }
    },
    "required": ["storeId", "storeName", "owner", "address", "price", "contactNumber", "id"]
  }
}
